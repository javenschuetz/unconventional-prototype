<UserControl x:Class="unconventional.keyboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:unconventional"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="450">
    <Grid OpacityMask="Black">
        <Image HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="450" Source="images/android-keyboard.png" Stretch="Fill" OpacityMask="Black"/>
        <Rectangle Fill="White" HorizontalAlignment="Left" Height="300" Stroke="Black" VerticalAlignment="Top" Width="450" Panel.ZIndex="-1"/>

    </Grid>
</UserControl>
